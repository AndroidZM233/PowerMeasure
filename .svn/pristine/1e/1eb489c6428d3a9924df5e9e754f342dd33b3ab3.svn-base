<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="speedata.com.powermeasure.activity.TroubleSecondAct">
    <common.view.CustomTitlebar
        android:id="@+id/titlebar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <LinearLayout
        android:padding="10dp"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:layout_weight="0.4"
            android:text="更换时间："
            android:layout_width="0dp"
            android:layout_height="wrap_content" />
        <TextView
            android:layout_weight="1"
            android:text="2016"
            android:id="@+id/tv_EqRepS_time"
            android:layout_width="0dp"
            android:layout_height="wrap_content" />
    </LinearLayout>
    <LinearLayout
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:layout_weight="0.4"
            android:text="设备编号："
            android:layout_width="0dp"
            android:layout_height="wrap_content" />
        <EditText
            android:singleLine="true"
            android:layout_weight="1"
            android:id="@+id/et_EqRepS_number"
            style="@style/main_button"
            android:layout_margin="10dp"
            android:layout_width="0dp"
            android:layout_height="50dp" />
    </LinearLayout>
    <LinearLayout
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:layout_weight="0.4"
            android:text="设备名称："
            android:layout_width="0dp"
            android:layout_height="wrap_content" />
        <EditText
            android:singleLine="true"
            android:layout_weight="1"
            android:id="@+id/et_EqRepS_name"
            style="@style/main_button"
            android:layout_margin="10dp"
            android:layout_width="0dp"
            android:layout_height="50dp" />
    </LinearLayout>
    <LinearLayout
        android:paddingBottom="5dp"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:layout_weight="0.4"
            android:text="注意事项："
            android:layout_width="0dp"
            android:layout_height="wrap_content" />
        <TextView
            android:text="azuo"
            android:layout_weight="1"
            android:id="@+id/tv_EqRepS_content"
            android:layout_width="0dp"
            android:layout_height="wrap_content" />
    </LinearLayout>
    <LinearLayout
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:layout_weight="0.4"
            android:text="备注："
            android:layout_width="0dp"
            android:layout_height="wrap_content" />
        <EditText
            android:singleLine="true"
            android:layout_weight="1"
            android:id="@+id/et_EqRepS_beizhu"
            style="@style/main_button"
            android:layout_margin="10dp"
            android:layout_width="0dp"
            android:layout_height="50dp" />
    </LinearLayout>

    <LinearLayout
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <Button
            android:background="@drawable/bg_alibuybutton"
            android:layout_weight="1"
            android:text="反馈"
            android:id="@+id/btn_EqRepS_fankui"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </LinearLayout>

</LinearLayout>
